start

new a number
new b number
new c number
new d number
new x number

set a 1000
set b 900
set c 400
set d 350
set x 586

if (x > a)
    log string "x > a"
close

if (a > x)
    if (x > b)
        log string "a > x > b"
    close
close

if (b > x)
    if (x > c)
        log string "b > x > c"
    close
close

if (c > x)
    if (x > d)
        log string "c > x > d"
    close
close

if (d > x)
    log string "x < d"
close

finish
